  },
  "author": "Kevin Regenrek",
  "license": "MIT",
  "devDependencies": {
    "consola": "^2.15.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/tailwindcss": "https://github.com/nuxt-community/tailwindcss-module#feat/tailwindcss2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "slugify": "^1.4.6",
    "tailwindcss": "^1.9.6",
    "vue-clickaway": "^2.2.2",
    "esno": "^0.3.0",
    "version-bump-prompt": "^6.1.0"
  }
