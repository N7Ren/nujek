{
  "name": "@nujek/monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "--preid",
  "description": "Nujek - Nuxt storyblok starter module",
  "scripts": {
    "publish:ci": "esno ./scripts/publish.js",
    "release": "esno ./scripts/release.js && git push --follow-tags"
